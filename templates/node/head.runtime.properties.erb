#
# this file is managed by puppet
# please do not change
#
druid.host=<%= @listen %>
druid.port=<%= @port %>
druid.zk.service.host=<%= @zk_host %>
druid.announcer.type=<%= @zk_announcer %>
druid.curator.compress=<%= @zk_compress %>

<% if @mysql_uri -%>
druid.db.connector.connectURI=<%= @mysql_uri %>
<% end -%>
<% if @mysql_user -%>
druid.db.connector.user=<%= @mysql_user %>
<% end -%>
<% if @mysql_pass -%>
druid.db.connector.password=<%= @mysql_pass %>
<% end -%>
<% if @use_validation_query -%>
druid.db.connector.useValidationQuery=<%= @use_validation_query %>
<% end -%>
<% if @storage_type -%>
druid.storage.type=<%= @storage_type %>
<% end -%>
druid.storage.bucket=<%= @storage_bucket %>
druid.storage.baseKey=<%= @storage_base_key %>
druid.s3.accessKey=<%= @s3_access_key %>
druid.s3.secretKey=<%= @s3_secret_key %>
<% if @extensions -%>
<% require 'json' -%>
druid.extensions.coordinates=<%= @extensions.to_json %>
<% end -%>
<% if @processing_buffer -%>
druid.processing.buffer.sizeBytes=<%= @processing_buffer %>
<% end -%>
<% if @processing_threads -%>
druid.processing.numThreads=<%= @processing_threads %>
<% end -%>
